{
    "exclude": [
        "package.json",
        "node_modules"
    ],
    "compilerOptions": {
        "lib": [
            "node",
            "dom",
            "es2015"
        ],
        "jsx": "react",
        "checkJs": true,
        "noEmit": true,
        "noImplicitAny": false,
        "allowSyntheticDefaultImports": true,
        "target": "es2013"
    }
}
